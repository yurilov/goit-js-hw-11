{{#each this}}
<div class="photo-card">
  <img src="{{webformatURL }}" alt="{{tags}}" loading="lazy" />
  <div class="info">
    <p class="info-item">
      <b>{{likes}} Likes</b>
    </p>
    <p class="info-item">
      <b>{{views}} Views</b>
    </p>
    <p class="info-item">
      <b>{{comments}} Comments</b>
    </p>
    <p class="info-item">
      <b>{{downloads}} Downloads</b>
    </p>
  </div>
</div>
     {{!-- <div class="card-img-top">
    <p class="country-list-item"><img src="{{flag}}" alt="{{name.official}}" width=45 height=30> {{name}}</p>
    </div> --}}
{{/each}}